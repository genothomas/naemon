#!/bin/sh
# prerm   script for naemon-thruk
#
# see: dh_installdeb(1)

set -e

case "$1" in
  remove|deconfigure)
    sed -i -e '/naemon-thruk.include/d' /etc/apache2/sites-available/*default*
  ;;

  upgrade|failed-upgrade)
  ;;

  *)
    echo "prerm called with unknown argument \`$1'" >&2
    exit 0
  ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
